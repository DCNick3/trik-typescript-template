{
    "compact": false,
    "presets": [
        [
            "@babel/preset-env",
            {
                "forceAllTransforms": true
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-regenerator",
            {
                "asyncGenerators": false,
                "generators": true,
                "async": false
            }
        ],
        [
          "@babel/plugin-transform-runtime",
          {
            "absoluteRuntime": true,
            "corejs": false,
            "helpers": true,
            "regenerator": true,
            "useESModules": true
          }
        ],
        "dynamic-import-node"
    ]
}
